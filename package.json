{
  "name": "crisli-picker",
  "version": "0.1.0",
  "description": "A customizable wheel picker for React applications",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "src/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./src/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src/types"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "type-check": "tsc --noEmit --project src/types/tsconfig.json",
    "prepublishOnly": "npm run build && npm run type-check"
  },
  "dependencies": {
    "react-spring": "^9.7.3",
    "@use-gesture/react": "^10.3.0",
    "date-fns": "^2.30.0"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "keywords": [
    "react",
    "date-picker",
    "time-picker",
    "wheel-picker",
    "calendar",
    "mobile",
    "responsive"
  ],
  "author": "Nazmul H. Rupok",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rupok/crisli.git"
  },
  "homepage": "https://crisli.app",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.15",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "rollup": "^2.79.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^5.0.0"
  }
}